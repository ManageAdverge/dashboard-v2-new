{
  "name": "analytics-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=test next dev",
    "build": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "lint": "next lint",
    "prisma:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "backup": "node dist/scripts/backup.js",
    "backup:auto": "ts-node scripts/auto-backup.ts",
    "backup:server": "ts-node scripts/server-backup.ts",
    "restore": "ts-node scripts/restore.ts",
    "backup:list": "ts-node scripts/restore.ts list",
    "backup:restore": "ts-node scripts/restore.ts restore",
    "test:dev": "cross-env NODE_ENV=test next dev",
    "test:build": "cross-env NODE_ENV=test next build",
    "test:start": "cross-env NODE_ENV=test next start",
    "prod:build": "cross-env NODE_ENV=production next build",
    "prod:start": "cross-env NODE_ENV=production next start",
    "test:setup": "ts-node --project tsconfig.scripts.json scripts/setup-test-db.ts",
    "build:scripts": "tsc --project tsconfig.scripts.json"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.815.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/x-date-pickers-pro": "^8.0.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@octokit/rest": "^21.1.1",
    "@prisma/client": "^6.6.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "chart.js": "^4.4.9",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "google-ads-api": "^19.0.4-rest-beta",
    "next": "14.1.0",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.15.3",
    "resend": "^4.4.1",
    "zod": "^3.24.3",
    "zustand": "^5.0.3"
  },
  "description": "A self-hosted analytics dashboard built with Next.js, Material-UI, and TypeScript. This dashboard provides a comprehensive view of your Google Ads campaign performance metrics.",
  "main": "next.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.15.21",
    "bcryptjs": "^3.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
